    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch TaskManager.Web2 (Local API)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/TaskManager.Web2/bin/Debug/net8.0/TaskManager.Web2.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/TaskManager.Web2",
            "stopAtEntry": false,
            "serverReadyAction": {
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
                "uriFormat": "%s",
                "action": "openExternally"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:5000",
                "Api__BaseUrl": "http://localhost:5001"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Launch TaskManager.Web2 (AWS API)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/TaskManager.Web2/bin/Debug/net8.0/TaskManager.Web2.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/TaskManager.Web2",
            "stopAtEntry": false,
            "serverReadyAction": {
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
                "uriFormat": "%s",
                "action": "openExternally"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:5000",
                "Api__BaseUrl": "https://0eclslvxej.execute-api.us-east-1.amazonaws.com/Prod/"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}